{
  "eslintConfig": {
    "extends": ["react-app", "shared-config", "prettier"],
    "rules": {
      "additional-rule": "warn"
    },
    "plugins": ["prettier"],
    "overrides": [
      {
        "files": ["**/*.ts?(x)"],
        "rules": {
          "additional-typescript-only-rule": "warn"
        }
      }
    ]
  }
}
